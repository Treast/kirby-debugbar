{
  "name": "treast/kirby-debugbar",
  "description": "PHP Debug Bar for KirbyCMS",
  "type": "kirby-plugin",
  "version": "1.1.0",
  "keywords": [
    "kirby",
    "kirbycms",
    "debugbar",
    "profiler",
    "debug"
  ],
  "homepage": "https://github.com/Treast/kirby-debugbar",
  "support": {
    "issues": "https://github.com/Treast/kirby-debugbar/issues",
    "source": "https://github.com/Treast/kirby-debugbar"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Treast\\KirbyDebugbar\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Vincent Riva",
      "email": "vincent.riva@outlook.fr",
      "homepage": "https://www.vincentriva.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "maximebf/debugbar": "^1.18.2",
    "getkirby/composer-installer": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}
